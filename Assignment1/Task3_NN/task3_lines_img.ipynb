    "\n",
    "Loss, Accuracy, itr = [], [], []\n",
    "init_op = tf.group(tf.global_variables_initializer(), tf.local_variables_initializer())\n",
    "\n",
    "model_saver = tf.train.Saver()\n",
    "\n",
    "sess = tf.Session()\n",
    "sess.run(init_op)\n",
    "\n",
    "input_classes = getInputClasses()\n",
    "\n",
    "# training the model\n",
    "for i in range(n_iterations):\n",
    "    itr.append(i + 1)\n",
    "    print i+1,\n",
    "    batch_X, batch_Y = getTrainingData(input_classes, batch_size)\n",
    "    sess.run(train_epoch, feed_dict={ip_data: batch_X, Y: batch_Y})\n",
    "    mini_loss, mini_accuracy = sess.run([cross_entropy, accuracy], feed_dict={ip_data: batch_X, Y: batch_Y})\n",
    "    Loss.append(mini_loss)\n",
    "    Accuracy.append(mini_accuracy)\n",
    "    if(i % 10 == 0):\n",
    "        print \"\\n\" + str(i + 1) + \" Loss: \" + str(mini_loss) + \" Accuracy: \" + str(mini_accuracy * 100.0)\n",
