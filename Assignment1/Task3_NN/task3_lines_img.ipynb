    "import tensorflow as tf\n",
    "import matplotlib.pyplot as plt\n",
    "import os\n",
    "import numpy as np\n",
    "import random\n",
    "import cv2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 122,
   "metadata": {},
   "outputs": [],
   "source": [
    "n_input = 2352   # input layer (28x28x3 pixels)\n",
    "n_hidden1 = 800 # 1st hidden layer\n",
    "# n_hidden2 = 256 # 2rd hidden layer\n",
    "n_output = 96   # output layer (96 classes)\n",
    "total_classes = 96 # 96 = 2 * 12 * 2 * 2\n",
    "op_folder = \"./LINE_IMG_DATA\"\n",
    "img_folder = \"../Task1_Images/img\"\n",
    "img_in_each_class = 1000\n",
    "training_samples = (img_in_each_class * 80) / 100\n",
    "TOTAL_TESTING_SAMPLES = 9600\n",
    "MAX_PIXEL_VALUE = 256.0\n",
    "MODEL_SAVE_PATH = \"./saved/model_lineImg\"\n",
    "\n",
    "learning_rate = 1e-2\n",
    "n_iterations = 1000\n",
    "batch_size = 96"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 123,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Get class label, label => [0, total_classes - 1] 0 indexed\n",
    "def getClassLabel(notation):\n",
    "    l, m, n, k = (int(x) for x in notation.split(\"_\"))\n",
    "    label = l * (2 * 12 * 2) + m * (12 * 2) + n * (2) + k\n",
    "    return int(label)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 124,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Reading the input classes\n",
    "def getInputClasses():\n",
    "    classes_folders = os.listdir(img_folder)\n",
    "    arr = []\n",
    "    for c in classes_folders:\n",
    "        label = getClassLabel(c)\n",
    "        arr.append([label, c])\n",
    "    arr = np.array(arr)\n",
    "    print arr\n",
    "    return arr"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 125,
   "metadata": {},
   "outputs": [],
   "source": [
    "def getTrainingData(input_classes, batch_size):\n",
    "    img_arr = []\n",
    "    Y_arr = np.zeros([batch_size, total_classes])\n",
    "    for i in range(batch_size):\n",
    "        random_index = random.randint(0, total_classes - 1)\n",
    "        label, class_folder = int(input_classes[random_index][0]), input_classes[random_index][1]\n",
    "        img = cv2.imread(img_folder + \"/\" + class_folder + \"/\" + class_folder + \"_\" + `(random.randint(0, training_samples - 1))` + \".jpg\")\n",
    "        img = img.flatten()\n",
    "        img = img / MAX_PIXEL_VALUE # Normalizing the pixels value to [0, 1]\n",
    "        Y_arr[i][label] = 1 # label => [0, total_classes - 1] 0 indexed\n",
    "        img_arr.append(img)\n",
    "    img_arr = np.array(img_arr)\n",
    "    return img_arr, Y_arr\n",
    "#     return one_hot_notation, img"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 126,
   "metadata": {},
   "outputs": [],
   "source": [
    "def getTestingData(input_classes, batch_size):\n",
    "    img_arr = []\n",
    "    Y_arr = np.zeros([batch_size, total_classes])\n",
    "    for i in range(batch_size):\n",
    "        random_index = random.randint(0, total_classes - 1)\n",
    "        label, class_folder = int(input_classes[random_index][0]), input_classes[random_index][1]\n",
    "        img = cv2.imread(img_folder + \"/\" + class_folder + \"/\" + class_folder + \"_\" + `(random.randint(training_samples, img_in_each_class - 1))` + \".jpg\")\n",
    "        img = img.flatten()\n",
    "        img = img / MAX_PIXEL_VALUE # Normalizing the pixels value to [0, 1]\n",
    "        Y_arr[i][label] = 1 # label => [0, total_classes - 1] 0 indexed\n",
    "        img_arr.append(img)\n",
    "    img_arr = np.array(img_arr)\n",
    "    return img_arr, Y_arr"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 127,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Getting the input data and the ground truth in 1-hot notation\n",
    "ip_data = tf.placeholder(\"float\", [None, n_input])\n",
    "Y = tf.placeholder(\"float\", [None, n_output])\n",
    "\n",
    "# Initializing the Weights matrices with a random normal distribution of S.D = 0.1\n",
    "W = {\n",
    "    'w1': tf.Variable(tf.truncated_normal([n_input, n_hidden1], stddev=0.1)),\n",
    "    'w2': tf.Variable(tf.truncated_normal([n_hidden1, n_output], stddev=0.1))\n",
    "}\n",
    "\n",
    "# Initializing the biases\n",
    "b = {\n",
    "    'b1': tf.Variable(tf.constant(0.2, shape=[n_hidden1])),\n",
    "    'b2': tf.Variable(tf.constant(0.2, shape=[n_output]))\n",
    "}\n",
    "\n",
    "a1 = tf.add(tf.matmul(ip_data, W['w1']), b['b1'])\n",
    "a1 = tf.nn.relu(a1)\n",
    "a2 = tf.add(tf.matmul(a1, W['w2']), b['b2'])\n",
    "# a2 = tf.nn.softmax(a2)\n",
    "# a3 = tf.add(tf.matmul(a2, W['w3']), b['b3'])\n",
    "# a3 = tf.nn.softmax(a3)\n",
    "\n",
    "# Cost function\n",
    "cross_entropy = tf.reduce_mean(tf.nn.softmax_cross_entropy_with_logits(labels=Y, logits=a2))\n",
    "train_epoch = tf.train.AdamOptimizer(learning_rate).minimize(cross_entropy)\n",
    "\n",
    "correct_pred = tf.equal(tf.argmax(a2, 1), tf.argmax(Y, 1))\n",
    "accuracy = tf.reduce_mean(tf.cast(correct_pred, tf.float32))\n",
    "\n",
    "prediction = tf.argmax(a2, 1)\n",
    "labels = tf.argmax(Y, 1)\n",
    "conf_matrix = tf.math.confusion_matrix(labels, prediction)\n",
    "precision = tf.metrics.precision(labels, prediction)\n",
    "recall = tf.metrics.recall(labels, prediction)"
   ]
  },
